

{%- style -%}
           .footer {
            margin-top: 0px;
            background-color: #ededed;
            color: #000000;
            
          }
/* Contact Button */
          .contactBtn{
            display: block;
            margin: auto;
            padding: 10px 10px 10px;
            background-color: black;
            color: #ffffff;
           
          }
          .contactBtn:hover{
            background-color: #ed3526;
            cursor: pointer;
            color: #ffffff;
          }
/* Divider */
          .dividerFooter{
            width: 100%;
            height: 5px;
           
     background-color: #b9baba; 
          }
/* Mother PP and Social Div  */
/* pp and tos */
          .pptcLinks{
            color: #000000;
            font-weight: 600;
          }
          .pptcLinks:hover{
            cursor: pointer;
            color: #ed3526;
          }
/* Social Icons */
          .socialIcons{
            border-radius: 5px;
            padding: 3px;
          }
          .socialIcons:hover{
            cursor: pointer;
            box-shadow: #ffffff 0px 3px 8px;
            color:white;
            background-color: white;
            
           
          }
  .socialSVGIcons{
    color: black;
    display: block-inline;
    width: 40%;
  }
       .footerLogo{
        width: 20%;
       
       }
  .mobileFooter{
