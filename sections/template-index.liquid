  }
  /* Hero Section */
        .heroDivIndex{
            background-color: #ffc82c;
        }
        .heroImg{
            width: 120%;
        }
        /* Product section */
        .psTwangImg{
                width: 50%;
        }
        .psTwangImgSm{
          width: 80%;
        }
        .psText{
            color: white;
            padding: 10px 0 10px;
          font-size: 2em;
        }
        #Pst1{
            background-color: #ffc526;

        }
        #Pst2{
            background-color: #ef8903;

        }
        #Pst3{
            background-color: #1b25ab;

