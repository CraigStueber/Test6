        }
        #Pst5{
            background-color: #39b555;

        }
        #Pst6{
            background-color: #03699a;

        }
        .psBtnDiv{
       
        }
        .psBtn{
            background-color: black;
            display: block;
            margin: auto;
        
            padding: 30px 40px 15px;
            border-radius: 5px;
            line-height: 0;
            color: white;
            color: white;
        }
        .psBtnLg{
            width: 30%;
        }
        .psBtnMob{
            width: 80%;
        }
        .psBtn:hover{
            color: black;
            background-color: #ffc526;
        
        }
        .psBtnDiv:hover{
        
        }
        /* Video Section */
        .videoDiv{
            position: relative;
            width: 100%;
            height: 800px;
            overflow: hidden;
        }

        .video{
            position: absolute;
          width: 100%;
           height: 100%;
           object-fit: cover;
            z-index: -1;
          
         
        }
        .vsImg{
            padding: 200px 0 0;
        }
        .vsImgMob{
            padding: 30px;
            background-color: black;
        }
  .heroTextIndex{
       font-family: Tungsten,Arial Narrow,Arial,sans-serif;
    color: black;
    background-color: #ffc82c;
  }
  .heroTextIndexSm{
    font-size: 2em;
    padding: 100px;
  }
  .heroTextIndexmd{
    font-size: 1.2em;
    padding-top: 30px;
  }
  .heroTextIndexlg{
    font-size: 1.4em;
    padding: 50px;
  }
  .heroTextIndexxl{
    font-size: 2.5em;
    padding: 50px;
    
  }
    .instImage{

    margin: 10px 0 10px;
  }
  .socialHeader{
    font-size: 3em;
    color: black;
  }
  .socialSubHeader{
    font-size: 2em;
    color: black;
  }
{% endstylesheet %}
 
 
